#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/solc@0.4.26/node_modules/solc/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/solc@0.4.26/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/solc@0.4.26/node_modules/solc/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/solc@0.4.26/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../solc@0.4.26/node_modules/solc/solcjs" "$@"
else
  exec node  "$basedir/../../../../../../solc@0.4.26/node_modules/solc/solcjs" "$@"
fi
