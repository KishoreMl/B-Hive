#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/window-size@0.2.0/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/window-size@0.2.0/node_modules:/mnt/c/Users/Barath/starstorage/contracts/contracts-api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../window-size@0.2.0/node_modules/window-size/cli.js" "$@"
else
  exec node  "$basedir/../../../../../window-size@0.2.0/node_modules/window-size/cli.js" "$@"
fi
